<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Restaurant  |--</th>
      <th>--|  Neigborhood</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% restaurants.forEach(function(r) { %>
      <tr>
        <td><%= r.name %></td>
        <td><%= r.neighborhood %></td>
        <td><%= r.address %></td>
        <td><a href="/restaurants/<%= r._id %>">Details</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
